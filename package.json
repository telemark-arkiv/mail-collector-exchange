{
  "name": "mail-collector-exchange",
  "version": "1.0.1",
  "description": "Collect a user's unread mails from exchange",
  "license": "MIT",
  "author": {
    "name": "Jonas Enge",
    "email": "jonas.enge@gmail.com",
    "url": "https://github.com/maccyber"
  },
  "bugs": {
    "url": "https://github.com/telemark/mail-collector-exchange/issues"
  },
  "homepage": "https://github.com/telemark/mail-collector-exchange",
  "main": "service.js",
  "engines": {
    "node": ">=4.6.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "start": "node service.js",
    "isolated": "MAIL_COLLECTOR_EXCHANGE_ISOLATED=1 node service.js",
    "setup": "npm install && node setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/telemark/mail-collector-exchange.git"
  },
  "devDependencies": {
    "coveralls": "2.11.14",
    "nsp": "2.6.2",
    "standard": "8.4.0",
    "tap": "8.0.0"
  },
  "dependencies": {
    "seneca": "3.2.2",
    "seneca-balance-client": "0.6.0",
    "seneca-mesh": "0.9.0",
    "ews-javascript-api": "0.6.0",
    "httpntlm": "1.7.1"
  }
}
